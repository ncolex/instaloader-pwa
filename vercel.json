{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "instaloader_pwa/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/css/(.*)", "dest": "/instaloader_pwa/css/$1" },
    { "src": "/js/(.*)", "dest": "/instaloader_pwa/js/$1" },
    { "src": "/images/(.*)", "dest": "/instaloader_pwa/images/$1" },
    { "src": "/(.*)", "dest": "/instaloader_pwa/$1" }
  ]
}